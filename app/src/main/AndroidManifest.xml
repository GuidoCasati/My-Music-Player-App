<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mymusicplayer">



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="musicplayerlayouts.songs"
            android:label="@string/songs" />
        <activity
            android:name="musicplayerlayouts.artists"
            android:label="@string/artists" />
        <activity
            android:name="musicplayerlayouts.Genres"
            android:label="@string/genres" />
        <activity
            android:name="musicplayerlayouts.albums"
            android:label="@string/albums" />
        <activity
            android:name="musicplayerlayouts.nowPlaying"
            android:label="@string/NowPlaying" />
    </application>



</manifest>